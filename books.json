import csv
import json

rows = []
with open("books.csv", newline="", encoding="utf-8") as f:
   reader = csv.DictReader(f)
   for r in reader:
   r["id"] = int(r["id"])
      r["pages"] = int(r["pages"])
      r["rating"] = int(r["rating"])
rows.append(r)
with open("books.json", "w", encoding="utf-8") as f:
    json.dump(rows, f, ensure_ascii=False, indent=2)
    print("Export Complete!")
